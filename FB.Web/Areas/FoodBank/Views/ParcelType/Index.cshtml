@{
    ViewBag.Title = "Pacel Type";
}

@section head{
    <link href="~/Content/datatables/dataTables.bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-switch.css" rel="stylesheet" />
    <link href="~/Scripts/plugins/alertify/css/alertify.css" rel="stylesheet" />
}
<div class="wrapper">
    <div class="">
        <div class="inner-bg">
            <section class="content-header">
                <h1>
                   Standard Parcel Type
                </h1>
            </section>
            <section class="content">
                <partial name="_Notification" />
                <div class="cmxform donor-form referrals-form">
                    <div class="table-top add-btn-main">
                        <a class="add-food-btn" href="@Url.Action("createedit")"><img src="/Content/images/add.png" alt="">Add Parcel Type</a>
                    </div>
                    <table id="grid-parceltype-master" class=" table table-bordered table-striped table-condensed food-table">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>S.No</th>
                                <th>Title</th>
                                <th>No. of Food Item</th>
                                <th>Created Date</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                        <tfoot></tfoot>
                    </table>
                </div>
            </section>
        </div>
    </div>
</div>
<partial name="_Modal" model='new Modal { ID="modal-delete-parceltype" , AreaLabeledId="modal-delete-parceltype-label" , Size=ModalSize.Medium }' />


@section scripts{
    <script src="~/Scripts/custom/parceltype/index.js"></script>
}
