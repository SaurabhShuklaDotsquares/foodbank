l-sm-4"></label>                                                <div class="col-lg-6 col-sm-6">                                                    <select asp-for="ParcelTypeId" class="form-control" asp-items="@((List<SelectListItem>)ViewBag.ParcelType)">                                                        <option value="">Select Parcel Type...</option>                                                    </select>                                                </div>                                            </div>                                        </div>                                        <div class="col-lg-6 col-sm-6">                                            <div class="col-lg-12 col-sm-12 col-md-offset-3">                                                <div class="form-group">                                                    <div class="col-lg-12 col-sm-12">                                                        <input asp-for="IncludeFoodDetails" type="checkbox" /> <label asp-for="IncludeFoodDetails"></label>                                                    </div>                                                </div>                                            </div>                                            <div class="col-lg-12 col-sm-12 col-md-offset-3">                                                <div class="form-group">                                                    <div class="col-lg-12 col-sm-12">                                                        <input asp-for="IsListByFamily" type="checkbox" /> <label asp-for="IsListByFamily"></label>                                                    </div>                                                </div>                                            </div>                                        </div>                                    </div>                                </div>                                @*<div class="row">                           