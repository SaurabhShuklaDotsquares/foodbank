@model FB.Dto.VolunteerReportDto@{ ViewData["Title"] = "Index";    Layout = "~/Views/Shared/_Layout.cshtml";}@section head{    <link href="~/Scripts/plugins/select2/select2.css" rel="stylesheet" />    <link href="~/Scripts/plugins/select2/select2-bootstrap.css" rel="stylesheet" />    <link href="~/Content/datatables/dataTables.bootstrap.css" rel="stylesheet" />    <link href="~/Content/multiple-select.css" rel="stylesheet" />}@section scripts{    <script src="~/Scripts/plugins/ckeditor1/ckeditor.js"></script>    <script src="~/Scripts/plugins/select2/select2.js"></script>    <script src="~/Scripts/jquery.bootstrap.wizard.js"></script>    <script src="~/Scripts/plugins/multiple-select/jquery.multiple.select.js"></script>    <script src="~/Scripts/custom/Report/Volunteer-Reports.js"></script>    <script src="~/Scripts/custom/Report/report-export-report.js"></script>}<style>    .rendertab2Data {        display: none    }
    .ms-drop input[type="checkbox"] {
        margin-top: -2px !important;
    }</style><section class="content-header">    <h1>        Volunteer Reports    </h1></section><section class="content report-common">    <div class="box">        <partial name="_Notification" />        <div id="frm-Role-list-of-people" class="cmxform">            <form asp-controller="VolunteerReports" asp-action="Index" asp-antiforgery="true">                <div class="box-body ">                    <div id="rootwizard" class="modal-body form-horizontal">                        <ul>                            <li><a href="#tab1" data-toggle="tab"><span class="bigW">Step-1</span><span class="smallW">1</span> </a></li>                            <li><a href="#tab2" data-toggle="tab"> <span class="bigW">Step-2</span><span class="smallW">2</span></a></li>                        </ul>                        <div class="tab-content" style="margin-top:20px;">                            <div class="row tab-pane" id="tab1">                                <div class="col-xs-12">                                    <div class="row  organisationDiv">                                        <div class="col-lg-12 col-sm-12">                                            <h3 class="page-header">Date range for shifts :</h3>                                            <div class="col-lg-6 col-sm-6">                                                <div class="form-group">                                                    <label asp-for="DateFrom" class="col-lg-3 col-sm-4"></label>                                                    <div class="col-lg-6 col-sm-6">                                                        <input type="date" asp-for="DateFrom" class="form-control dateSelect" />                                                        <span asp-validation-for="DateFrom" class="text-red"></span>                                                    </div>                                                </div>                                            </div>                                            <div class="col-lg-6 col-sm-6">                                                <div class="form-group">                                                    <label asp-for="DateTo" class="col-lg-3 col-sm-4"></label>                                                    <div class="col-lg-6 col-sm-6">                                                        <input type="date" asp-for="DateTo" class="form-control dateSelect" />                                                        <span asp-validation-for="DateTo" class="text-red"></span>                                                    </div>                                                </div>                                            </div>                                        </div>                                    </div>                                    <br />                                    <br />                                    <div class="row">                                        <div class="col-lg-12 col-sm-12">                                            <div class="col-lg-6 col-sm-6">                                                <div class="form-group">                                                    <label asp-for="VolunterrsIds" class="col-lg-3 col-sm-4"></label>                                                    <div class="col-lg-6 col-sm-6">                                                        @*<select class="form-control" multiple="multiple" id="volunteer" name="volunteer"></select>*@                                                        <select id="volunteer" name="volunteer" multiple="multiple" asp-items="@((List<SelectListItem>) ViewBag.VolunteersList)">                                                        </select>                                                        <input type="hidden" asp-for="VolunterrsIds" />                                                        <span asp-validation-for="VolunterrsIds" class="text-red"></span>                                                    </div>                                                </div>                                            </div>                                        </div>                                    </div>                                    <br />                                    <br />                                    <div class="row">                                        <div class="col-lg-12 col-sm-12">                                            <div class="col-lg-6 col-sm-6">                                                <div class="form-group">                                                    <label asp-for="ShiftTypeIds" class="col-lg-3 col-sm-4"></label>                                                    <div class="col-lg-6 col-sm-6">                                                        <select id="shiftType" name="shiftType" multiple="multiple" asp-items="@((List<SelectListItem>) ViewBag.ShiftType)">                                                        </select>                                                        <input type="hidden" asp-for="ShiftTypeIds" />                                                        <span asp-validation-for="ShiftTypeIds" class="text-red"></span>                                                    </div>                                                </div>                                            </div>                                        </div>                                    </div>                                    <div class="row">                                        <div class="col-lg-12 col-sm-12">                                            <h3 class="page-header">Sort By:-</h3>                                            <div class="col-lg-6 col-sm-6">                                                <div class="form-group">                                                    <div class="col-lg-3 col-sm-3">                                                        <label class="control-label">                                                            <input asp-for="sortBy" type="radio" value="@((int)SortBy.Date)" checked="checked" />                                                            @(SortBy.Date.GetDescription())                                                        </label>                                                    </div>                                                </div>                                                <div class="form-group">                                                    <div class="col-lg-3 col-sm-3">                                                        <label class="control-label">                                                            <input asp-for="sortBy" type="radio" value="@((int)SortBy.Volunteer)" />                                                            @(SortBy.Volunteer.GetDescription())                                                        </label>                                                    </div>                                                </div>                                            </div>                                        </div>                                    </div>                                </div>                            </div>                            <div class="row tab-pane" id="tab2">                                <div class="col-xs-12">                                    <div class="row">                                        <div class="col-lg-12 col-sm-12">                                            <div id="div-step-three" class="rendertab2Data">                                                <partial name="_ExportReferrerTab3.cshtml" />                                                @*@await Component.InvokeAsync("ColumnSelection", new { id = ReportType.RoleListofPeople })*@                                            </div>                                        </div>                                    </div>                                </div>                            </div>                            <ul class="pager wizard">                                <li class="previous first" style="display:none;"><a href="javascript:void(0);">First</a></li>                                <li class="previous"><a href="javascript:void(0);">Previous</a></li>                                <li class="next last" style="display:none;"><a href="javascript:void(0);">Last</a></li>                                <li class="next"><a href="javascript:void(0);">Next</a></li>                            </ul>                        </div>                    </div>                    <div class="img-loading-div" style="display:none;">                        <img src="@Url.Content("~/content/images/loader.gif")" />                    </div>                </div>            </form>        </div>    </div></section>