@model FB.Dto.GrantorReportDto@{ ViewData["Title"] = "Index";    Layout = "~/Views/Shared/_Layout.cshtml";}@section head{    <link href="~/Scripts/plugins/select2/select2.css" rel="stylesheet" />    <link href="~/Scripts/plugins/select2/select2-bootstrap.css" rel="stylesheet" />    <link href="~/Content/datatables/dataTables.bootstrap.css" rel="stylesheet" />    <link href="~/Content/multiple-select.css" rel="stylesheet" />}@section scripts{    <script src="~/Scripts/plugins/select2/select2.js"></script>    <script src="~/Scripts/jquery.bootstrap.wizard.js"></script>    <script src="~/Scripts/plugins/multiple-select/jquery.multiple.select.js"></script>    <script src="~/Scripts/custom/Report/Grantors-Report.js"></script>    <script src="~/Scripts/custom/Report/report-export-report.js"></script>}<style>    .rendertab2Data {        display: none    }
    .ms-drop input[type="checkbox"] {
        margin-top: -2px !important;
    }</style><section class="content-header">    <h1>        Grantors Report    </h1></section><section class="content report-common">    <div class="box">        <partial name="_Notification" />        <div id="frm-Role-list-of-people" class="cmxform">            <form asp-controller="GrantorsReport" asp-action="Index" asp-antiforgery="true">                <div class="box-body ">                    <div id="rootwizard" class="modal-body form-horizontal">                        <ul>                            <li><a href="#tab1" data-toggle="tab"><span class="bigW">Step-1</span><span class="smallW">1</span> </a></li>                            <li><a href="#tab2" data-toggle="tab"> <span class="bigW">Step-2</span><span class="smallW">2</span></a></li>                        </ul>                        <div class="tab-content" style="margin-top:20px;">                            <div class="row tab-pane" id="tab1">                                <div class="col-xs-12">                                    <div class="row  organisationDiv">                                        <div class="col-lg-12 col-sm-12">                                            <h3 class="page-header"></h3>                                            <div class="col-lg-12 col-sm-12">                                                <div class="form-group">                                                    <label asp-for="GrantorId" class="col-lg-2 col-sm-2"></label>                                                    <div class="col-lg-8 col-sm-8">                                                        <select id="grantor" name="grantor"  multiple="multiple" asp-items="@((List<SelectListItem>) ViewBag.grantorList)">                                                        </select>                                                        <input type="hidden" asp-for="GrantorId" />                                                        <span asp-validation-for="GrantorId" class="text-red"></span>                                                    </div>                                                </div>                                            </div>                                        </div>                                    </div>                                </div>                            </div>                            <div class="row tab-pane" id="tab2">                                <div class="col-xs-12">                                    <div class="row">                                        <div class="col-lg-12 col-sm-12">                                            <div id="div-step-three" class="rendertab2Data">                                                <partial name="_ExportReferrerTab3.cshtml" />                                                @*@await Component.InvokeAsync("ColumnSelection", new { id = ReportType.RoleListofPeople })*@                                            </div>                                        </div>                                    </div>                                </div>                            </div>                            <ul class="pager wizard">                                <li class="previous first" style="display:none;"><a href="javascript:void(0);">First</a></li>                                <li class="previous"><a href="javascript:void(0);">Previous</a></li>                                <li class="next last" style="display:none;"><a href="javascript:void(0);">Last</a></li>                                <li class="next"><a href="javascript:void(0);">Next</a></li>                            </ul>                        </div>                    </div>                    <div class="img-loading-div" style="display:none;">                        <img src="@Url.Content("~/content/images/loader.gif")" />                    </div>                </div>            </form>        </div>    </div></section>